{
  "name": "nexus-explorer-next",
  "private": true,
  "version": "1.0.4",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "serve": "next build && next start",
    "lint": "next lint",
    "format": "prettier -w ./src",
    "prepare": "husky install",
    "clean:cache": "rimraf .next",
    "clean": "rimraf node_modules",
    "clean:deep": "rimraf .next && rimraf dist && rimraf node_modules",
    "overrideChangelog": "gren changelog --override"
  },
  "dependencies": {
    "apexcharts": "^3.35.0",
    "axios": "^0.27.2",
    "http-proxy-middleware": "^2.0.6",
    "js-cookie": "^3.0.1",
    "next": "12.1.5",
    "prop-types": "^15.8.1",
    "react": "18.1.0",
    "react-apexcharts": "^1.4.0",
    "react-dom": "18.1.0",
    "react-icons": "^4.3.1",
    "react-qr-code": "^2.0.7",
    "react-query": "^3.38.0",
    "react-table": "^7.7.0",
    "sass": "^1.51.0",
    "sharp": "^0.30.7"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "danger": "^11.1.1",
    "eslint": "8.14.0",
    "eslint-config-next": "12.1.5",
    "eslint-config-prettier": "^8.5.0",
    "github-release-notes": "^0.17.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "prettier": "2.6.2",
    "rimraf": "^3.0.2",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ],
    "**/*{.css,scss}": [
      "prettier --write --ignore-unknown"
    ]
  }
}